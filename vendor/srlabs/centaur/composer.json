{
    "name": "srlabs/centaur",
    "description": "An opinionated implementation of Cartalyst's Sentinel for Laravel 5",
    "keywords": ["laravel", "sentinel", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Durham",
            "email": "ryan@stagerightlabs.com",
            "homepage": "http://stagerightlabs.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Centaur\\": "src/"
        }
    },
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0",
        "cartalyst/sentinel": "^3.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0",
        "mockery/mockery": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Centaur\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "vinkla/hashids": "A Hashids bridge for Laravel"
    }
}
